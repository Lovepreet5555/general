// Generated by Selenium IDE
const { Builder, By, Key, until } = require('selenium-webdriver');
const assert = require('assert');

describe('test21', function () {
  this.timeout(80000);
  let driver;
  let vars;

  beforeEach(async function () {
    driver = await new Builder().forBrowser('chrome').build();
    vars = {};
  });

  afterEach(async function () {
    if (driver) {
      await driver.quit();
    }
  });

  it('test21', async function () {
    await driver.get("https://www.xenonstack.com/");
    await driver.manage().window().maximize();

    // Interact with platform element
    const platformElement = await driver.wait(until.elementLocated(By.css(".platform-li > p")), 10000);
    await driver.wait(until.elementIsVisible(platformElement), 10000);
    await driver.executeScript("arguments[0].scrollIntoView(true);", platformElement);
    await platformElement.click();

    // Interact with navigation elements
    const navElement = await driver.wait(until.elementLocated(By.css(".nav-li:nth-child(2) > p")), 10000);
    await driver.wait(until.elementIsVisible(navElement), 10000);
    await driver.executeScript("arguments[0].scrollIntoView(true);", navElement);
    await navElement.click();

    // Fill out form fields
    await driver.findElement(By.id("Firstname")).sendKeys("abc");
    await driver.findElement(By.id("Lastname")).sendKeys("abc");
    await driver.findElement(By.id("emailid")).sendKeys("love@xenonstack.com");
    await driver.findElement(By.id("contactnumber")).sendKeys("1212121212");
    await driver.findElement(By.id("companyname")).sendKeys("xenonstack");

    // Select dropdown option
    const dropdown = await driver.findElement(By.id("enterpriseIndustry"));
    await dropdown.findElement(By.xpath("//option[. = 'Software Development']")).click();

    // Perform actions on dropdown
    await driver.actions({ bridge: true }).moveToElement(dropdown).clickAndHold().perform();
    await driver.actions({ bridge: true }).moveToElement(dropdown).release().perform();

    // Click next-step buttons and interact with other elements
    await driver.findElement(By.css(".next-step-button:nth-child(1) > p")).click();
    await driver.findElement(By.css(".next-step-button:nth-child(2) > p")).click();
    await driver.findElement(By.css("#aiTransformation .answers:nth-child(5) > p")).click();
    await driver.findElement(By.css(".next-step-button:nth-child(2) > p")).click();
    await driver.findElement(By.css("#aiUsecase .answers:nth-child(4) > p")).click();
    await driver.findElement(By.css(".next-step-button:nth-child(2) > p")).click();
    await driver.findElement(By.css("#solution .answers:nth-child(4) > p")).click();
    await driver.findElement(By.css("#dataPlatform .answers:nth-child(6) > p")).click();
    await driver.findElement(By.css("#companyInfra .answers:nth-child(3) > p")).click();
    await driver.findElement(By.css("#primaryChallenge .answers:nth-child(2) > p")).click();
    await driver.findElement(By.css(".answers:nth-child(14)")).click();
    await driver.findElement(By.css("#agenticaiPlatform .answers:nth-child(6)")).click();
    await driver.findElement(By.css(".next-step-button:nth-child(2) > p")).click();
    await driver.findElement(By.css(".next-step-button:nth-child(2) > p")).click();
    await driver.findElement(By.css("#companySegment .answers:nth-child(3)")).click();
    await driver.findElement(By.css(".next-step-button:nth-child(2) > p")).click();
  });
}); 



